<?xml version='1.0' encoding='utf-8'?><widget android-versionCode="10" id="com.github.beaufortfrancois.cog_chrome_app" version="0.0.12.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cog - System Info Viewer</name>
    <description>__MSG_appDescription__</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Author Name &lt;a@b.com></author>
    <content src="plugins/cordova-plugin-chrome-apps-bootstrap/chromeapp.html"/>
    <plugin name="cordova-plugin-whitelist" version="1"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
        
        
        
        
        
        
        
        
    <icon src="www/assets/icon_144.png"/><icon src="www/assets/icon_16.png" width="16"/><icon src="www/assets/icon_36.png" width="36"/><icon src="www/assets/icon_48.png" width="48"/><icon src="www/assets/icon_78.png" width="78"/><icon src="www/assets/icon_96.png" width="96"/><icon src="www/assets/icon_128.png" width="128"/><icon src="www/assets/icon_144.png" width="144"/></platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
        
        
        
        
        
        
        
        
    <icon src="www/assets/icon_144.png"/><icon src="www/assets/icon_16.png" width="16"/><icon src="www/assets/icon_36.png" width="36"/><icon src="www/assets/icon_48.png" width="48"/><icon src="www/assets/icon_78.png" width="78"/><icon src="www/assets/icon_96.png" width="96"/><icon src="www/assets/icon_128.png" width="128"/><icon src="www/assets/icon_144.png" width="144"/></platform>
    <preference name="android-minSdkVersion" value="14"/>
    
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect"/>
    <preference name="xwalkMode" value="embedded"/>
</widget>